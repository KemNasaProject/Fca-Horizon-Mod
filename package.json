  "repository": {
    "type": "git",
    "url": "git://github.com/HarryWakazaki/Fca-Horizon-Mod.git"
  },
  "dependencies": {
    "aes-js": "latest",
    "assert": "latest",
    "bluebird": "latest",
    "chalk": "5.2.0",
    "cheerio": "latest",
    "crypto": "latest",
    "crypto-js": "latest",
    "express": "latest",
    "file-url": "^3.0.0",
    "got": "^11.8.3",
    "horizon-sp": "file:Horizon_Package/horizon-sp",
